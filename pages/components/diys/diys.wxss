/* pages/index/index.wxss */
page{
  width: 750rpx;
  overflow: hidden;
  position: relative;
  background: #ededed;
}

.canvass{
    position: absolute;
  
}

.block{
    width: 300rpx;
    height: 300rpx;
    background-color: rebeccapurple;
    position: absolute;
    display: block; 
}

.blocks{
    position: absolute;
}
.block{
    position: relative;
}
.close{
    position: absolute;
    top: -30rpx;
    left: -10rpx;
    z-index: 1;
}
.close image{
  height: 45rpx;
  width: 45rpx;
  position: absolute;
  left: -14rpx;
  top: 10rpx;
}
.rotate{
    position: absolute;
    bottom:35rpx;
    right:-24rpx;
    z-index: 1;
}

.nav-phone{
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  z-index: 990;
  background-color: #ffffff;
  border-bottom: 1rpx solid #eee;
}
.nav-phone image{
  width:50rpx;
  height:50rpx;
  padding-right: 10rpx;
}
.type-info{
  font-size:28rpx;
  display: flex;
}
.type-info text{
  color: #ffd100;
  margin-left: 50rpx;
} 
.order-btn{
  width:150rpx;
  height:100%;
  font-size:25rpx;
  line-height:40rpx;
  text-align:center;
  background-color:#d563a0;
  color:#fff;

}
.orlinfo{
  display: flex;
  align-items: center;
  
}
.diys-box{
  height: 766rpx;
}
.diys-phonebg{
  /* background-color: #d563a0; */
  height: 800rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.nav-list{
  display: flex;
  justify-content:space-around;
  position:fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  flex-direction: column;
}
.nav-list-item{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size:22rpx;

}

.nav-list-item>image{
  
  width: 55rpx;
  height: 55rpx;
}
.select-item{
  height:194rpx;
  background-color: #999;
}
.nav-lists{
  display: flex;
  justify-content:space-around;
  padding-top: 10rpx;
}
.texture{
  display: flex;
  padding-top: 10rpx;
  flex-direction: column;
}
.items-view{
  color:#000;
  background-color:#e2e2e2!important;
  border:1px solid #e2e2e2;
  border-radius:20rpx;
  min-width:70rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align:center;
  font-size: 30rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  margin-right: 10rpx;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
}
.select-name{
  font-size: 26rpx;
  width:100rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
}
.line{
  font-size:20rpx;
  line-height:50rpx;
  color:#e2e2e2;
  margin-right: 20rpx;
}
.select-list-box{
    display: flex;
   align-items: center;
   min-height: 50rpx;
  flex-wrap: wrap;
}
.type-info>view{
  font-size: 26rpx;
  width: 373rpx;
  overflow: hidden;
  white-space: nowrap; 
  text-overflow: ellipsis; 
}
.add-phone{
  padding-top: 20rpx;

}
.item-phone{
  background-color:#fff;
  width:129rpx;
  height:150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 20rpx;
  
}
.item-phone image{
  width:24rpx;
  height:24rpx;
}
.select-add-phone{
  width:129rpx;
  height:150rpx;
  border:1px solid #d563a0;
  margin-left: 20rpx;
}
.add-box{
  display: flex;
}
.icon-move{
  width: 50rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
 
}
.diys-phonebg{

}
.diy-box{
  position: absolute;
  top: 0;
  margin-top: 50rpx;
  left: 0;
  margin-left: 214rpx;
}
.phone-mode{
  position: absolute;
}
.fonst-input{
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 50rpx;
}
.fonst-input input{
  background-color: #fff;
}

.color-list{
  display: flex;
  flex-wrap: wrap;
}
.color-list>view{
  flex: 1;
  width:67rpx;
  height:51rpx;
  margin-left:0rpx;

}
.select-color{
  display: flex;
  flex-direction: column;
}
fonst-size{
  display: inline-block;
  text-align: center;


}
.font-size-show{
  display: inline-block;
  font-size: 20rpx;
  line-height: 30rpx;
}
.text-styles{
  -webkit-writing-mode: vertical-rl;

}
.fonst-type{
  display: flex;
  align-items: center;
}
.box-mes{

  width: 100rpx;
  height: 100rpx;
  border-radius:50%;
  font-size: 27rpx;
  background-color: #fff; 
  line-height: 100rpx; 
  text-align: center;
}
.scroll-view_H{
  white-space: nowrap;
  display: flex;
}
.scroll-view_H text{
  margin: 0px;
  padding: 0px;
  display: inline-block;
  margin-right: 30rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  font-family: Fonttype0;
}

