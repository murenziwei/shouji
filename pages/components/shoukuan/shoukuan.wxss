.money-list{
  border-bottom:20rpx solid #f1f1f1;
  width:100%;
  box-sizing: border-box;

}
.ml-top,.money-list:last-child{
  border-bottom:1px solid #f1f1f1;
}
.m-b-item,.ml-top{
  display:flex;
  align-items:center;
  justify-content: space-between;
  padding:20rpx;
  width:100%;
  box-sizing: border-box;
}
.m-text{
  font-size:40rpx;
  color:#999;
}
.ml-bottom{
  font-size:30rpx;
  color:#999;
}
.mbi-active{
  font-size:25rpx;
  color:#ffa415;
}
.ml-money{
  color:#ffa415;
}


.m-icon{
  font-size:40rpx;
  color:#999;
  padding-right:10rpx;
}
.mbi-icon{
  font-size:30rpx;
  padding-right:10rpx;
}