<!--pages/commission/commission.wxml-->
<view class='bg'>
  <image src='../../img/bg1.png' style='width:100%;display:block' mode='widthFix'></image>
  <view class='bg-itme'>
    <view class='money'>
      {{brokerage.withdraw}}
    </view>
    <view class='noney-tip'>
      可提现佣金
    </view>
  </view>
</view>

<view class='item-fix'>

  <view class='fix-item'>
    <text>{{brokerage.total_money}}</text>
    <text class='fix-text'>累计佣金</text>
  </view>

  <view class='fix-item'>
    <text>1230.00</text>
      <text class='fix-text'>已提佣金</text>
  </view>
      
  <view class='fix-item'>
    <text>1230.00</text>
    <text class='fix-text'>待打款佣金</text>
  </view>


</view>

<view class='list' catchtap='showinfobtn'>
  <text>提现明细</text>
  <image src='../../img/back.png'></image>
</view>
<view class='list' catchtap='morepagebtn'>
  <text>用户须知 </text>
  <image src='../../img/back.png'></image>
</view>

<view class='selectbtn'>
提交
</view>
